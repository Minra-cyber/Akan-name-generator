        <!-- GTM Data Analytics Listener -->
        <script type="text/javascript">
            jQuery.ajax({
                url: 'https://gateway.bibliocommons.com/v2/libraries/kcls/sessions/current',
                dataType: 'json',
                method: 'GET',
                xhrFields: { withCredentials: true },
            }).done(function (p) {
                var current_user = p['auth']['currentUserId'];
                if (current_user) {
                    window.dataLayer.push({'bc':{'encodedUserId':p['entities']['users'][current_user]['encodedId']}});
                }
            });

            function pushDataAnalytics(data) {
                jQuery.each(data, function () {
                    window.dataLayer.push(shapeAnalytics(this));
                });
            }

            function pushToDataLayer() {
                pushDataAnalytics(JSON.parse(jQuery(this).attr("data-analytics").replace(/&quot;/g, '"')));
            }

            function shapeAnalytics (data_analytics) {
                var formed_data = data_analytics;
                var analyticsShape = ['gtmCategory', 'gtmAction', 'gtmLabel', 'gtmValue', 'gtmContentType', 'gtmContainerName', 'gtmContentCreator', 'gtmCreatorType', 'gtmCardTitle'];
                // taken from core repository bc_google_tag_manager_data_api.js
                for (var i = 0; i < analyticsShape.length; i++) {
                    if (!formed_data.payload.hasOwnProperty(analyticsShape[i])) {
                        formed_data.payload[analyticsShape[i]] = undefined;
                    } else if (['gtmLabel', 'gtmContainerName', 'gtmCardTitle'].indexOf(analyticsShape[i]) > -1) {
                        // removing escaped single quotes
                        formed_data.payload[analyticsShape[i]] = formed_data.payload[analyticsShape[i]].replace(/&#039;/g,"'");
                    }
                }
                return formed_data;
            }

                        function addAnalyticsDebounce(func, wait, immediate) {
                var timeout;
                return function() {
                    var context = this, args = arguments;
                    var later = function() {
                        timeout = null;
                        if (!immediate) func.apply(context, args);
                    };
                    var callNow = immediate && !timeout;
                    clearTimeout(timeout);
                    timeout = setTimeout(later, wait);
                    if (callNow) func.apply(context, args);
                };
            };

            var addAnalytics = addAnalyticsDebounce(function () {
                var links = jQuery(this).find('[data-analytics]').not('[data-analytics-event-added]');
                links.each(function () {
                    jQuery(this)
                        .on('click', pushToDataLayer)
                        .on('contextmenu', pushToDataLayer)
                        .attr('data-analytics-event-added', 'true');
                });
            }, 100);

            // Adds datalayer push to elements with data-analytics when the dom is changed.
            jQuery('.biblioweb_container').bind('DOMSubtreeModified', addAnalytics);

            if (typeof(window.bc_Environment.data_analytics) !== 'undefined') {
                jQuery.each(window.bc_Environment.data_analytics, function() {
                    window.dataLayer.push(shapeAnalytics(JSON.parse(this.replace(/&quot;/g, '"'))));
                });
            }
        </script>
        <!-- End GTM Data Analytics Listener -->
